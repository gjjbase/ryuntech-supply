#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../_scss-bundle@2.5.1@scss-bundle/dist/bundle-cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../../_scss-bundle@2.5.1@scss-bundle/dist/bundle-cli.js" "$@"
  ret=$?
fi
exit $ret
