#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../_dart-sass@1.17.3@dart-sass/sass.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../../_dart-sass@1.17.3@dart-sass/sass.js" "$@"
  ret=$?
fi
exit $ret
